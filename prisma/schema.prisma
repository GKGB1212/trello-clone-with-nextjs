generator client {
  provider        = "prisma-client-js"
  // The following line will need to be added if
  // using driver adapters.
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Board {
  id    String @id @default(uuid())
  title String
}
